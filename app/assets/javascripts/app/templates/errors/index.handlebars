<h1>Erreurs</h1>

<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<th>Code</th>
			<th>Message</th>
			<th colspan="2">Date</th>
		</tr>
	</thead>
	<tbody>
		{{#each view.errors}}
		<tr>
			<td>{{this.code}}</td>
			<td>{{this.message}}</td>
			<td>{{this.date}}</td>
			<td>{{this.hour}}</td>
		</tr>
		{{/each}}
	</tbody>
</table>
